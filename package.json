{
  "name": "akylas-nativescript-vue",
  "version": "2.1.8",
  "description": "NativeScript and Vue integration",
  "main": "dist/index.js",
  "files": [
    "dist/index.js",
    "index.d.ts"
  ],
  "typings": "index.d.ts",
  "scripts": {
    "test": "jest",
    "tdd": "jest --watch",
    "samples": "node build/sample-runner.js",
    "dev": "rollup -c build/config.js -w --o samples/app/nativescript-vue.js --environment TARGET:akylas-nativescript-vue",
    "dev:dist": "rollup -c build/config.js -w --o dist/index.js --environment TARGET:akylas-nativescript-vue",
    "build": "node build/build.js",
    "build:docs": "cd docs && npm run build",
    "prettier": "prettier --no-semi --single-quote --write \"{{platform,__test__}/**/*.js,samples/app/*.js}\"",
    "release": "node build/releaser.js",
    "release:notes": "node build/gen-release-notes.js",
    "changelog": "conventional-changelog --release-count 0 --outfile CHANGELOG.md --preset angular",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Akylas/nativescript-vue.git"
  },
  "keywords": [
    "vuejs",
    "nativescript",
    "integration"
  ],
  "author": "Igor Randjelovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Akylas/nativescript-vue/issues"
  },
  "homepage": "https://github.com/Akylas/nativescript-vue#readme",
  "nativescript": {
    "platforms": {
      "android": "4.2.0",
      "ios": "4.2.0"
    },
    "plugin": {
      "vue": "true",
      "pan": "false",
      "core3": "true",
      "category": "Developer"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-env": "7.3.4",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-jest": "^24.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "chalk": "^2.4.2",
    "commitizen": "^3.0.7",
    "conventional-changelog-cli": "^2.0.12",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "inquirer": "^6.2.2",
    "jest": "^24.1.0",
    "jest-junit": "^6.3.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "rollup": "^1.3.1",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-flow-no-whitespace": "^1.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-resolve-aliases": "^0.2.0",
    "rollup-watch": "^4.3.1",
    "semver": "^5.6.0",
    "set-value": "^3.0.0",
    "tns-core-modules": "5.2.1",
    "util-inspect": "^0.1.8",
    "vue": "^2.6.10"
  },
  "jest": {
    "verbose": true,
    "modulePaths": [
      "<rootDir>/platform/nativescript"
    ],
    "collectCoverageFrom": [
      "platform/**/*.js",
      "!**/node_modules/**"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/samples"
    ],
    "collectCoverage": true,
    "testURL": "http://localhost"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "linters": {
      "{{platform,__test__}/**/*.js,samples/app/*.js}": [
        "prettier --write",
        "git add"
      ],
      "docs/src/**/*.js": [
        "npm run build:docs",
        "git add"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
